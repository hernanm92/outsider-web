<!--=== Team v1 ===-->
<div class="container content-md team-v1">
    <ul class="list-unstyled row">
        <li class="col-sm-3 col-xs-6 md-margin-bottom-30" ng-repeat="rider in riders">
            <div class="team-img">
                <!--<img class="img-responsive" src={{rider.photo_url}} alt="">-->
                <div style="background-image: url({{rider.photo_url}}); height:260px; background-position:center;background-size: cover;"></div>
                <ul>
                    <li><a href="{{rider.twitter}}" target="_blank"><i class="icon-custom icon-sm rounded-x fa fa-twitter"></i></a></li>
                    <li><a href="{{rider.facebook}}" target="_blank"><i class="icon-custom icon-sm rounded-x fa fa-facebook"></i></a></li>
                    <li><a href="{{rider.instagram}}" target="_blank"><i class="icon-custom icon-sm rounded-x fa fa-instagram"></i></a></li>
                </ul>
            </div>
            <h3><a href="#/teams/{{rider.alias}}" style="text-decoration: none;">{{rider.name}}</a></h3> <!--hacer que el nombre sea un link-->
            <h4>/ {{rider.sport}}</h4>
            <p>{{rider.description}}...</p>
        </li>
    </ul>
</div>
<!--=== End Team v1 ===-->

